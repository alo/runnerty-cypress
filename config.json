{
  "$schema": "https://raw.githubusercontent.com/runnerty/schemas/master/schemas/3.1/config.json",
  "general": {
    "runnerty.io": {
      "apikey": "YOUR_APIKEY_HERE"
    }
  },
  "triggers": [
    {
      "id": "schedule_default",
      "type": "@runnerty-trigger-schedule"
    }
  ],
  "executors": [
    {
      "id": "shell_default",
      "type": "@runnerty-executor-shell"
    },
    {
      "id": "cypress_default",
      "type": "@runnerty-executor-cypress"
    }
  ],
  "notifiers": [
    {
      "id": "console_default",
      "type": "@runnerty-notifier-console"
    }
  ]
}
